//cra
yarn create react-app crowdcoin-web --template typescript

//web3
yarn add web3

//ganache-cli
yarn add ganache-cli
yarn add --dev @types/ganache-cli

//mocha
yarn add mocha
yarn add --dev @types/mocha

//solc
yarn add solc

//@truffle/hdwallet-provider
yarn add @truffle/hdwallet-provider


//GIT
git init
git add .
git commit -m "Initializing Repo"
git branch -M main
git remote add origin https://github.com/adolfredo-coneo/crowdcoin-web.git
git push -u origin main
